{
  "name": "codexten/yii-cron",
  "type": "library",
  "description": "Metronic Theme integration",
  "require": {
    "codexten/yii-web": "~2.0",
    "mult1mate/cron-manager": "*"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\cron\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "cron": "config/cron.php",
      "console": "config/console.php",
      "migrationNamespaces": "config/migrationNamespaces.php"
    }
  }
}
