{
  "name": "codexten/yii-cron",
  "type": "library",
  "description": "Metronic Theme integration",
  "require": {
    "voodoo-mobile/yii2-cron": "*"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\cron\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "metronic": [
        "$vue",
        "src/config/metronic.php"
      ],
      "params": "src/config/params.php",
      "console": "src/config/console.php",
      "hidev": "src/hidev/config/hidev.php"
    }
  }
}
